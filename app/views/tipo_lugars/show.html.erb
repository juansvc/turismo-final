<%- model_class = TipoLugar -%>
<div class="page-header">
  <h1><%=t '.title', :default => TipoLugar %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nombre_tipo) %></dt>
      <dd><pre class="prettyprint"><%= @tipo_lugar.nombre_tipo %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tipo_de_lugar_creado) %>
      <dd><pre class="prettyprint"><%=l @tipo_lugar.created_at, format: :long %></pre></dd>
      </dt>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("AtrÃ¡s")),
              tipo_lugars_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("Editar")),
              edit_tipo_lugar_path(@tipo_lugar), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("Eliminar")),
              tipo_lugar_path(@tipo_lugar),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

