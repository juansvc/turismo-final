<%- model_class = Transporte -%>
<div class="page-header">
  <h1><%=t '.title', :default => Transporte %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lugar_salida) %></dt>
      <dd><pre class="prettyprint"><%= @transporte.lugar_salida %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lugar_llegada) %></dt>
      <dd><pre class="prettyprint"><%= @transporte.lugar_llegada %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:hora_salida) %></dt>
      <dd><pre class="prettyprint"><%= @transporte.hora_salida %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:hora_llegada) %></dt>
      <dd><pre class="prettyprint"><%= @transporte.hora_llegada %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fecha) %></dt>
      <dd><pre class="prettyprint"><%= @transporte.fecha %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nombre_transporte_id) %></dt>
      <dd><pre class="prettyprint"><%= @transporte.nombre_transporte_id %></pre></dd>
      <dd><pre class="prettyprint"><%= @transporte.nombre_transporte.nombre %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("AtrÃ¡s")),
              transportes_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("Editar")),
              edit_transporte_path(@transporte), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("Eliminar")),
              transporte_path(@transporte),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

