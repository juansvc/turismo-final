<%- model_class = NombreTransporte -%>
<div class="page-header">
  <h1><%=t '.title', :default => NombreTransporte %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nombre) %></dt>
      <dd><pre class="prettyprint"><%= @nombre_transporte.nombre %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("AtrÃ¡s")),
              nombre_transportes_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("Editar")),
              edit_nombre_transporte_path(@nombre_transporte), :class => 'btn btn-default' %>
